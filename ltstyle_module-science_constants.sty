% Set of constants values
% based on scipy.cstes
% all given in SI units

\newcommand{\pivalue}{3.141592653589793}

\newcommand{\DeltaNuCsvalue}{9192631770} % transition hyperfine du césium 133
\newcommand{\cvalue}{299792458} % vitesse de la lumière dans le vide
\newcommand{\hvalue}{6.62607015e-34} % constante de Planck
\newcommand{\evalue}{1.602176634e-19} % charge élémentaire
\newcommand{\kBvalue}{1.380649e-23} % constante de Boltzmann
\newcommand{\NAvalue}{6.02214076e23} % nombre d'Avogadro
\newcommand{\Kcdvalue}{683} % efficacité lumineuse à 540 THz

\newcommand{\hbarvalue}{(\hvalue/(2*\pivalue))} % constante de Planck réduite
\newcommand{\Faradayvalue}{(\NAvalue*\evalue)} % Faraday
\newcommand{\RGPvalue}{(\NAvalue*\kBvalue)} % constante du gaz parfait
\newcommand{\muzerovalue}{(4*\pivalue*1e-7)} % perméabilité du vide
\newcommand{\epsilonzerovalue}{(1/(\muzerovalue*\cvalue^2))} % permittivité du vide
\newcommand{\eVvalue}{\evalue}

\newcommand{\RHvalue}{1.0973731568508e7} % constante de Rydberg
\newcommand{\Gvalue}{6.67430e-11} % constante gravitationnelle
\newcommand{\Wienvalue}{(\hvalue*\cvalue/(4.96511423174427*\kBvalue))} % constante de Wien
\newcommand{\Stefanvalue}{(2*\pivalue^5/15*\kBvalue^4/(\hvalue^3*\cvalue^2))} % constante de Stefan-Boltzmann

\newcommand{\MTerrevalue}{5.97217e24} % masse de la Terre
\newcommand{\RTerrevalue}{6.3781e6} % rayon de la Terre au niveau de l'équateur
\newcommand{\gvalue}{9.80665} % accélération de pesanteur sur Terre
\newcommand{\MSoleilvalue}{1.989e30} % masse du Soleil
\newcommand{\RSoleilvalue}{6955e5} % rayon du Soleil
\newcommand{\MLunevalue}{7.35e22} % masse de la Lune
\newcommand{\RLunevalue}{1737.4e3} % rayon de la Lune
\newcommand{\DTerreSoleilvalue}{1.49597870700e11} % distance Terre-Soleil
\newcommand{\uniteastrovalue}{\DTerreSoleilvalue}
\newcommand{\DTerreLunevalue}{380000e3} % distance Terre-Lune
\newcommand{\TsurfSoleilvalue}{5770} % température de surface du Soleil
\newcommand{\anneelumierevalue}{(\cvalue*365.25*24*60*60)} % distance égale à une année lumière
\newcommand{\parsecvalue}{(648000/\pivalue*\uniteastrovalue)}

\newcommand{\Patmvalue}{1013.25e2} % pression atmosphérique au niveau de la mer
\newcommand{\VmGPvalue}{(\RGPvalue*273.15/\Patmvalue)} % volume molaire du gaz parfait

\newcommand{\DeltaFheauvalue}{333.55e3} % at Patm
\newcommand{\DeltaVheauvalue}{2264.76e3} % at Patm

\newcommand{\Melectronvalue}{9.1093837015e-31} % masse de l'électron
\newcommand{\Mprotonvalue}{1.67262192369e-27} % masse du proton
\newcommand{\Mneutronvalue}{1.67492749804e-27} % masse du neutron
\newcommand{\Matomiquevalue}{(1/(\NAvalue*1e3))} % masse atomique = masse C12 / 12
\newcommand{\RBohrvalue}{0.529177210903e-10} % rayon de Borh
\newcommand{\lambdaComptonelevalue}{2.4263102387e-12} % longueur d'onde de Compton pour l'électron

\newcommand{\millimetreHgvalue}{133.322} % millimètre de mercure
\newcommand{\calorievalue}{4.185} % une calorie en joules

\newcommand{\RTlnonF}{\kBvalue*(273.15+25)/\evalue*math.log(10)} % RT/(Na e)*ln(10) = kBT/e*ln(10) value for Nernst

\newcommand{\europarkWh}{0.2276}
