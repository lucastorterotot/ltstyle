\lstset{#1}
}
{}

% Python for external files
\ifthenelse{\equal{\useminted}{True}}
           {
             \newcommand\inputpython[2][]{{
             \inputminted[breaklines, bgcolor=\codecolortheme lstcolbg, #1]{python}{#2}}}
           }
           {
             \newcommand\inputpython[2][]{{
                 \pythonstyle
                 \lstinputlisting[language = Python, #1]{#2}}}
           }
           
% Python for inline
\ifthenelse{\equal{\useminted}{True}}
           {
             \newcommand\inlinepython[1]{{\mintinline{python}{#1}}}
           }
           {
             \newcommand\inlinepython[1]{{\pythonstyle\lstinline@#1@}}
           }

% C++ environment
\lstnewenvironment{C++}[1][]
{
\cppstyle
\lstset{#1}
}
{}

% C++ for external files
\ifthenelse{\equal{\useminted}{True}}
           {
             \newcommand\inputcpp[2][]{{
                 \inputminted[breaklines, bgcolor=\codecolortheme lstcolbg, #1]{c++}{#2}}}
           }
           {
             \newcommand\inputcpp[2][]{{
                 \cppstyle
                 \lstinputlisting[language = C++, #1]{#2}}}
           }
           
% C++ for inline
\ifthenelse{\equal{\useminted}{True}}
           {
             \newcommand\inlinecpp[1]{{\mintinline{c++}{#1}}}
           }
           {
             \newcommand\inlinecpp[1]{{\cppstyle\lstinline@#1@}}
           }

% LaTeX environment
\lstnewenvironment{latex}[1][]
{
\latexstyle
\lstset{#1}
}
{}

% LaTeX for external files
\ifthenelse{\equal{\useminted}{True}}
           {
             \newcommand\inputlatex[2][]{{
                 \inputminted[breaklines, bgcolor=\codecolortheme lstcolbg, #1]{latex}{#2}}}
           }
           {
             \newcommand\inputlatex[2][]{{
                 \latexstyle
                 \lstinputlisting[#1]{#2}}}
           }
           
% LaTeX for inline
\ifthenelse{\equal{\useminted}{True}}
           {           
             \newcommand\inlinelatex[1]{{\mintinline{latex}{#1}}}
           }
           {         
             \newcommand\inlinelatex[1]{{\latexstyle\lstinline!#1!}}
           }  
